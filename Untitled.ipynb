      "\n",
      "interface 10.10.10.1\n",
      "\tdescription \"Link to Main router\"\n",
      "\tip address 10.10.10.1/24\n",
      "\n"
     ]
    }
   ],
   "source": [
    "from string import Template\n",
    "\n",
    "template = Template(\"\"\"\n",
    "interface $ip\n",
    "\\tdescription \"Link to $desc\"\n",
    "\\tip address $ip/24\n",
    "\"\"\")\n",
    "\n",
    "data = {\n",
    "    \"ip\": \"10.10.10.1\",\n",
    "    \"desc\": \"Main router\"\n",
    "}\n",
    "\n",
    "result = template.substitute(data)\n",
    "print(result)\n"
   ]
  },
